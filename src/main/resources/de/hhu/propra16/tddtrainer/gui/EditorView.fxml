<AnchorPane prefHeight="335.0" prefWidth="640.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.hhu.propra16.tddtrainer.gui.EditorViewController">
	<children>
		<SplitPane dividerPositions="0.5" layoutX="-1.0" layoutY="1.0" prefHeight="315.0" prefWidth="640.0" style="-fx-background-color: white;" stylesheets="@editorView.css" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="55.0">
			<items>
				<ScrollPane fitToHeight="true" fitToWidth="true" minWidth="140.0">
					<content>
						<AnchorPane fx:id="codePane" style="-fx-background-color: white;">
							<children>
                        <HBox alignment="CENTER_LEFT" layoutX="14.0" layoutY="-1.0" spacing="10.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="0.0">
                           <children>
      								<Label text="Code:" textFill="#6f8391">
      									<font>
      										<Font size="21.0" />
      									</font>
      								</Label>
                              <Label fx:id="codeLabel" textFill="#6f8391">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
							</children>
						</AnchorPane>
					</content>
				</ScrollPane>
				<ScrollPane fitToHeight="true" fitToWidth="true" minWidth="140.0">
					<content>
						<AnchorPane fx:id="testPane" style="-fx-background-color: white;">
							<children>
                        <HBox alignment="CENTER_LEFT" layoutX="15.0" spacing="10.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="0.0">
                           <children>
      								<Label text="Tests:" textFill="#6f8391">
      									<font>
      										<Font size="21.0" />
      									</font>
      								</Label>
                              <Label fx:id="testLabel" textFill="#6f8391">
                                 <font>
                                    <Font size="18.0" />
                                 </font>
                              </Label>
                           </children>
                        </HBox>
							</children>
						</AnchorPane>
					</content>
				</ScrollPane>
			</items>
		</SplitPane>
		<Button fx:id="nextStepButton" disable="true" layoutX="553.0" layoutY="10.0" mnemonicParsing="false" onAction="#handleNextStep" style="-fx-background-color: #6f8391;" text="%nextStep" textFill="WHITE" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="10.0">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Button>
		<Label layoutX="380.0" layoutY="13.0" text="STATUS:" textFill="#6f8391" AnchorPane.rightAnchor="190.0" AnchorPane.topAnchor="10.0">
			<font>
				<Font name="System Bold" size="18.0" />
			</font>
		</Label>
      <Label fx:id="statusLabel" layoutX="460.0" layoutY="8.0" prefHeight="36.0" prefWidth="85.0" stylesheets="@editorView.css" textFill="#6f8391" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="5.0">
         <font>
            <Font name="System Bold" size="25.0" />
         </font>
      </Label>
      <HBox layoutX="15.0" layoutY="10.0" minWidth="100.0" prefHeight="31.0" prefWidth="356.0" spacing="10.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="270.0" AnchorPane.topAnchor="10.0">
         <children>
            <Label text="%exercise" textFill="#6f8391" HBox.hgrow="NEVER">
               <font>
                  <Font name="System Bold" size="21.0" />
               </font>
            </Label>
            <Label fx:id="exerciseLabel" textFill="#6f8391">
               <font>
                  <Font size="21.0" />
               </font>
            </Label>
         </children>
      </HBox>
	</children>
